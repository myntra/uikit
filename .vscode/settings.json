{
  "cSpell.words": [
    "JABONG",
    "Scroller",
    "clickable",
    "lockfile",
    "myntra",
    "uikit"
  ],
  "eslint.enable": false,
  "eslint.validate": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "markdownlint.config": {
    "no-inline-html": {
      "allowed_elements": ["summary", "details", "code"]
    },
    "list-marker-space": {
      "ul_multi": 2,
      "ol_multi": 2,
      "ol_single": 1,
      "ul_single": 1
    }
  }
}
